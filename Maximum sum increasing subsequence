class Solution
{
	public int maxSumIS(int arr[], int n)  
	{  
	    //code here.
	    if(n==1)
	    return arr[0];
	    int max =0;
	    int[] dp = new int[n];
	    for(int i=0;i<n;i++){
	        dp[i] = arr[i];
	        for(int j=0;j<i;j++){
	            if(arr[i]>arr[j])
	            dp[i] = Math.max(dp[i],dp[j]+arr[i]);
	        }
	    }
	    for(int i=0;i<dp.length;i++)
	    max = Math.max(max,dp[i]);
	    return max;
	}  
}
