class Solution {
    public int lengthOfLIS(int[] nums) {
        int[] dp = new int[nums.length];
        for (int i = nums.length - 1; i >= 0; i--) {
            int num = nums[i];
            dp[i] = 0;
            for (int d = i + 1; d < nums.length; d++) {
                if (nums[i] < nums[d]) {
                    dp[i] = Math.max(dp[i], dp[d]);
                }
            }
            dp[i] = dp[i] + 1;
        }
        int max = 0;
        for (int i = 0; i < dp.length; i++) {
            max = Math.max(dp[i], max);
        }
        return max;
    }
}
